    "make-aliases": "node network/makeAliases.js",
    "clear": "rimraf dist",
    "start:server": "node index.js",
    "start:nodemon": "nodemon --exec \"run-s start:build-server start:server\"",
    "start:build-server": "webpack --config webpack.server.js",
    "start:build-client": "webpack serve --config webpack.client.js",
    "start": "cross-env NODE_ENV=development npm-run-all clear --parallel start:build-client start:nodemon",
    "lint": "eslint \"src/**/*.@(tsx|ts|jsx|js)\" --fix && prettier --check \"src/**/*.@(tsx|ts|jsx|js)\" --write",
    "test": "jest --config .jest/config.json --runInBand",
    "generate": "plop"
  },
